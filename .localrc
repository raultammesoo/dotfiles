# ~/.config/.localrc


# your project folder that we can `c [tab]` to
export PROJECTS_CD="/Users/sander/Library/CloudStorage/GoogleDrive-sander.tammesoo@gmail.com/My\ Drive/9\ -\ Arhiiv/910\ -\ Personal/919.\ Self\ dev/919.2.\ SW\ Projects"
export PROJECTS="/Users/sander/Library/CloudStorage/GoogleDrive-sander.tammesoo@gmail.com/My Drive/9 - Arhiiv/910 - Personal/919. Self dev/919.2. SW Projects"

export PYENV_ROOT="$XDG_CONFIG_HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"

export NVM_DIR="$XDG_CONFIG_HOME/nvm"
export NVM_HOMEBREW="$(brew --prefix nvm)/nvm.sh"
[ -s "$NVM_HOMEBREW" ] && \. "$NVM_HOMEBREW"
[ -s "/opt/homebrew/opt/nvm/nvm.sh" ] && \. "/opt/homebrew/opt/nvm/nvm.sh"  # This loads nvm
[ -s "/opt/homebrew/opt/nvm/etc/bash_completion.d/nvm" ] && \. "/opt/homebrew/opt/nvm/etc/bash_completion.d/nvm"  # This loads nvm bash_completion

#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="$XDG_CONFIG_HOME/.sdkman"
[[ -s "$SDKMAN_DIR/bin/sdkman-init.sh" ]] && source "$SDKMAN_DIR/bin/sdkman-init.sh"
